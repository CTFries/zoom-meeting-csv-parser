{
  "check-coverage": true,
  "per-file": true,
  "lines": 99,
  "produce-source-map": true,
  "statements": 99,
  "functions": 99,
  "branches": 99,
  "include": ["lib/*.js"],
  "exclude": ["**/*.spec.js", "spec", "coverage"],
  "reporter": ["lcov", "text-summary", "html"],
  "cache": true,
  "all": true,
  "report-dir": "./coverage",
  "tmp-directory": ".nyc_output",
  "extension": [".js"]
}
